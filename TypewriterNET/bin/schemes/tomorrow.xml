<?xml version="1.0" encoding="UTF-8" ?>
<scheme>
	<defColor name="bg" value="#2d2d2d"/>
	<defColor name="currentLine" value="#393939"/>
	<defColor name="selection" value="#515151"/>
	<defColor name="fg" value="#cccccc"/>
	<defColor name="comment" value="#999999"/>
	<defColor name="red" value="#f2777a"/>
	<defColor name="orange" value="#f99157"/>
	<defColor name="yellow" value="#ffcc66"/>
	<defColor name="green" value="#99cc99"/>
	<defColor name="aqua" value="#66cccc"/>
	<defColor name="blue" value="#6699cc"/>
	<defColor name="purple" value="#cc99cc"/>
	
	<color name="bg" value="bg"/>
	<color name="fg" value="fg"/>
	<color name="lineBg" value="currentLine"/>
	<color name="lineNumberBg" value="currentLine"/>
	<color name="lineNumberFg" value="#888888"/>
	<color name="selectionBrush" value="selection"/>
	<color name="selectionPen" value="selection"/>
	<color name="markPen" value="selection"/>
	<color name="mainCaret" value="#ffffff"/>
	<color name="caret" value="#80ffff"/>
	
	<color name="printMargin" value="#808080"/>
	<color name="tabsBg" value="#505050"/>
	<color name="tabsFg" value="#eeeeee"/>
	<color name="tabsSelectedBg" value="#808080"/>
	<color name="tabsSelectedFg" value="#ffffff"/>
	
	<width name="mainCaret" value="2"/>
	<width name="caret" value="2"/>
	
	<color name="splitterBg" value="#505050"/>
	<color name="splitterLine" value="#505050"/>
	<color name="scrollBg" value="currentLine"/>
	<color name="scrollThumb" value="#808080"/>
	<color name="scrollThumbHover" value="#acacac"/>
	<color name="scrollArrow" value="fg"/>
	<color name="scrollArrowHover" value="#ffffff"/>
	
	<style name="dsNormal" color="fg"/>
	<style name="dsKeyword" color="purple"/>
	<style name="dsDataType" color="red"/>
	<style name="dsDataType2" color="purple"/>
	<style name="dsDecVal" color="orange"/>
	<style name="dsBaseN" color="orange"/>
	<style name="dsFloat" color="orange"/>
	<style name="dsChar" color="red"/>
	<style name="dsString" color="green"/>
	<style name="dsComment" color="comment"/>
	<style name="dsDocStart" color="comment"/>
	<style name="dsOthers" color="yellow"/>
	<style name="dsAlert" color="red" bold="true"/>
	<style name="dsFunction" color="blue"/>
	<style name="dsRegionMarker" color="fg"/>
	<style name="dsError" color="red" underline="true"/>
	
	<style name="dsOperator" color="fg"/>
	<style name="dsConstructor" color="yellow"/>
	<style name="dsNormal2" color="fg"/>
	<style name="dsKeyword2" color="purple"/>
	<style name="dsString2" color="green"/>
	<style name="dsOthers2" color="yellow"/>
	
	<style name="dsDocHtml1" color="aqua"/>
	<style name="dsDocHtml2" color="aqua"/>
	<style name="dsDocHtml3" color="aqua"/>
	<style name="dsDocEntities" color="aqua"/>
	<style name="dsDocTag" color="comment" bold="true"/>
	<style name="dsDocWord" color="comment"/>
	<style name="dsDocDescription"  color="comment" bold="true"/>
	<style name="dsDocCustomTag" color="comment"/>
	<style name="dsDocAlertTag1" color="aqua" bold="true"/>
	<style name="dsDocAlertTag2" color="aqua" bold="true"/>
	<style name="dsDocAlertTag3" color="aqua" bold="true"/>
	<style name="dsDocAlert1" color="aqua" bold="true"/>
	<style name="dsDocAlert2" color="aqua" bold="true"/>
	<style name="dsDocAlert3" color="aqua" bold="true"/>
	<style name="dsDocInner" color="comment" italic="true"/>
</scheme>
